        <Label x:Name="Lakier" Content="Lakier" HorizontalAlignment="Left" Margin="45,154,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="samochodCMB" HorizontalAlignment="Left" Margin="65,56,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="samochodCMB_SelectionChanged"/>
        <ComboBox x:Name="silnikCMB" HorizontalAlignment="Left" Margin="65,132,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="silnikCMB_SelectionChanged"/>
        <ComboBox x:Name="lakierCMB" HorizontalAlignment="Left" Margin="66,201,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="lakierCMB_SelectionChanged"/>
